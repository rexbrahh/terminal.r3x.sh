{
  "name": "terminal.r3x.sh",
  "version": "1.0.0",
  "description": "Unix-like terminal interface for web browsing",
  "type": "module",
  "scripts": {
    "start": "python3 -m http.server 8000",
    "dev": "python3 scripts/dev_server.py --port 8000",
    "dev:isolated": "python3 scripts/dev_server.py --port 8000 --isolated --coep credentialless",
    "serve": "npx serve ."
  },
  "keywords": [
    "terminal",
    "web-terminal",
    "unix",
    "command-line",
    "portfolio"
  ],
