# Minimal BusyBox config for Emscripten (browser)
# Generated manually; review and adjust per BusyBox version

# General
CONFIG_DESKTOP=n
CONFIG_STATIC=y
CONFIG_PIE=n
CONFIG_NOMMU=y
CONFIG_FEATURE_USE_BSS_TAIL=y
CONFIG_FEATURE_BUFFERS_USE_MALLOC=y
CONFIG_LONG_OPTS=y

# Shell
CONFIG_ASH=y
CONFIG_ASH_OPTIMIZE_FOR_SIZE=y
CONFIG_ASH_ECHO=y
CONFIG_ASH_PRINTF=y
CONFIG_ASH_TEST=y
CONFIG_ASH_CMDCMD=y
CONFIG_FEATURE_SH_STANDALONE=y
CONFIG_FEATURE_SH_NOFORK=y

# Core utilities
CONFIG_CAT=y
CONFIG_ECHO=y
CONFIG_PRINTF=y
CONFIG_WC=y
CONFIG_HEAD=y
CONFIG_TAIL=y
CONFIG_TR=y
CONFIG_GREP=y
CONFIG_SED=y
CONFIG_AWK=y
CONFIG_HEXDUMP=y
CONFIG_STRINGS=y
CONFIG_LESS=y
CONFIG_FIND=y
CONFIG_XARGS=y
CONFIG_CP=y
CONFIG_MV=y
CONFIG_RM=y
CONFIG_MKDIR=y
CONFIG_RMDIR=y
CONFIG_TOUCH=y
CONFIG_DF=y
CONFIG_DU=y

# Compression
CONFIG_GZIP=y
CONFIG_GUNZIP=y

# Disable unsupported features for browser
CONFIG_MOUNT=n
CONFIG_UMOUNT=n
CONFIG_LOGIN=n
CONFIG_PASSWD=n
CONFIG_TELNET=n
CONFIG_HTTPD=n
CONFIG_IFCONFIG=n
CONFIG_ROUTE=n
CONFIG_UDHCPC=n
CONFIG_HUSH=n
